{
  "name": "youtubewindowedfullscreen",
  "version": "1.0.0",
  "scripts": {
    "build": "bash ./scripts/build-extension.sh",
    "chrome-deploy": "chrome-webstore-upload upload --extension-id $EXTENSION_ID --source ./deployments/Chrome.zip --auto-publish",
    "firefox-deploy": "web-ext-submit --source-dir ./deployments/Firefox.zip"
  },
  "author": "Navi Jador",
  "devDependencies": {
    "chrome-webstore-upload-cli": "^2.1.0",
    "web-ext": "^7.1.1",
    "web-ext-submit": "^7.1.1"
  }
}
